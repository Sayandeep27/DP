
#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int f(int i,int j,string &s,string &t)
{
  // base case
    
    if(i<0 or j<0) return 0;

    
    
    if(s[i]==t[j])
    {
        return  1+f(i-1,j-1,s,t);
    }
    
    return  max(f(i-1,j,s,t),f(i,j-1,s,t));
    
    
    
}



int main() {
    
  string s1 = "abcde";
  string s2 = "bdgek";
  
  int n=s1.size();
  int m=s2.size();
  
  vector<vector<int>>dp(n+1,vector<int>(m+1,-1));

  cout << "The Longest Common Subsequence is ----";
  int ans=f(n-1,m-1,s1,s2);
  
  cout<<ans<<endl;

int len=ans;

int i=n;
int j=m;

int index=len-1;

string str="";

for(int idx=0;idx<len;idx++)
{
    str+='?';  // dummy string
}

while(i>0 and j>0)
{
    if(s1[i-1]==s2[j-1])
    {
        str[index]=s1[i-1];
        index--;
        i--;
        j--;
    }
    else if(s1[i-1]>s2[j-1])
    {
        i--;
    }
    else
    {
        j--;
    }
}


cout<<str<<endl;
 
 
    
    
    return 0;
}



----------------------------Tabulation-----------------------------------------------------------------------------------------------------


void lcs(string &s1,string &s2)
{
    
    int n=s1.size();
    int m=s2.size();
    
    vector<vector<int>>dp(n+1,vector<int>(m+1,0));
    
    
    for(int i=1;i<=n;i++)
    {
        for(int j=1;j<=m;j++)
        {
            if(s1[i-1]==s2[j-1])
            {
                dp[i][j]=1+dp[i-1][j-1];
            }
            else
            {
                dp[i][j]=0+max(dp[i-1][j],dp[i][j-1]);
            }
        }
    }
    
    int len=dp[n][m];
    int i=n;
    int j=m;
    
    int index=len-1;
    
    string str="";
    
    for(int idx=1;idx<=len;idx++)
    {
        str+="?";  // dummy string
    }
    
    
    
    while(i>0 and j>0)
    {
        if(s1[i-1]==s2[j-1])
        {
            str[index]=s1[i-1];
            index--;
            i--;
            j--;
        }
        else if(s1[i-1]>s2[j-1])
        {
            i--;
        }
        else
        {
            j--;
        }
    }
    
    
    
    cout<<str<<endl;
    
    
    
}



int main() {
    
  string s1 = "abcde";
  string s2 = "bdgek";

  cout << "The Longest Common Subsequence is ----";
  lcs(s1, s2);

    return 0;
}
